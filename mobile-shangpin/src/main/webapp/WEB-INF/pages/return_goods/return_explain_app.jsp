<rapid:override name="content">
    <div class="alContent" >
      <!--内容区域 start-->
      <div class="return-explain-pop js-return-explain-pop">
      	<%-- <a class="close-btn"><img src="${cdn:js(pageContext.request)}/styles/shangpin/images/return_goods/close_btn.png" /></a> --%>
