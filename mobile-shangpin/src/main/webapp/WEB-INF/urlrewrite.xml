<urlrewrite>
	<outbound-rule encodefirst="true">
		<name>过滤存在jsessionid参数</name>
		<note>
			过滤存在jsessionid参数URL
		</note>
		<from>^(.*);jsessionid=.*$</from>
		<to>$1</to>
	</outbound-rule>
	<outbound-rule match-type="wildcard">
		<from>*;jsessionid=*</from>
		<to>$1</to>
	</outbound-rule>
